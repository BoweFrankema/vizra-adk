{
  "name": "aaronlumsden/laravel-agent-adk",
  "description": "A Laravel package for building AI-driven agents, inspired by Google ADK.",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron Lumsden",
      "email": "aaronlumsden@me.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^12.0",
    "prism-php/prism": "^0.68.1",
    "league/csv": "^9.23"
  },
  "autoload": {
    "psr-4": {
      "AaronLumsden\\LaravelAgentADK\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AaronLumsden\\LaravelAgentADK\\Providers\\AgentServiceProvider"
      ],
      "aliases": {
        "Agent": "AaronLumsden\\LaravelAgentADK\\Facades\\Agent"
      }
    }
  }
}
